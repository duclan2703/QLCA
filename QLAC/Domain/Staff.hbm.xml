<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="QLAC.Domain.Staff, QLAC" dynamic-update="true" table="Staff" >
    <id name="ID" column="ID" type="Int32" unsaved-value="0">
      <generator class="native">
        <param name="sequence">SEQ_CUSTOMER</param>
      </generator>
    </id>
    <property name="Name" column="Name" type="string" length="50" />
    <property name="DateOfBirth" column="DateOfBirth" type="datetime"/>
    <property name="Position" column="Position" type="string" length="30" />
    <property name="Username" column="Username" type="string" length="15" />
    <property name="Password" column="Password" type="string" length="15" />
    <property name="Office" column="Office" type="string" length="50"/>
  </class>

</hibernate-mapping>
